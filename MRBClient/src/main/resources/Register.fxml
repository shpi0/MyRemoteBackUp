<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="globParent" alignment="center" xmlns="http://javafx.com/javafx/8.0.112-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="RegisterController">
    <TextField fx:id="login" maxWidth="200" promptText="Login">
      <VBox.margin>
         <Insets bottom="10.0" />
      </VBox.margin></TextField>
    <PasswordField fx:id="password" maxWidth="200" promptText="Password" />
    <PasswordField fx:id="password2" maxWidth="200" promptText="Password again" />
    <Button fx:id="regBtn" onAction="#reg" prefWidth="200" text="Register" />
    <Button onAction="#close" prefWidth="200" text="Cancel">
      <opaqueInsets>
         <Insets />
      </opaqueInsets>
      <VBox.margin>
         <Insets top="10.0" />
      </VBox.margin></Button>
</VBox>
